{
  "name": "inertia-adapter-solid",
  "version": "1.0.0-beta.1",
  "license": "MIT",
  "description": "The SolidJS adapter for Inertia.js",
  "contributors": [
    "Jorge Gonz√°lez <yo@jorgeglz.io>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iksaku/inertia-adapter-solid.git"
  },
  "bugs": {
    "url": "https://github.com/iksaku/inertia-adapter-solid/issues"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/types/server.d.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.js"
    }
  },
  "scripts": {
    "dev": "./build.js --watch",
    "build": "rm -rf dist && ./build.js && tsc --emitDeclarationOnly",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "@inertiajs/core": "^2.2.11",
    "@solid-primitives/deep": "^0.3.3",
    "@solid-primitives/memo": "^1.4.3",
    "es-toolkit": "^1.41.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.29.7",
    "@solidjs/meta": "^0.29.4",
    "esbuild": "^0.25.11",
    "esbuild-node-externals": "^1.18.0",
    "solid-js": "^1.9.9",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@solidjs/meta": "^0.29.3",
    "solid-js": "^1.8.0"
  }
}
